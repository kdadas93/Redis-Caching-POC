            <stringProp name="script">import java.util.concurrent.ConcurrentHashMap

// Retrieve the map from the JMeter variables
def counts = vars.getObject(&quot;counts&quot;)
if (counts == null) {
    // If the map doesn&apos;t exist yet, create it
    counts = new ConcurrentHashMap()
    vars.putObject(&quot;counts&quot;, counts)
}

// Get all response headers
def allHeaders = prev.getResponseHeaders()

// Find the value of &quot;Who-handled-me&quot; header
def matcher = allHeaders =~ /Who-handled-me: (.*)/
if(matcher.find()) {
    def whoHandledMe = matcher[0][1]

    // Update the count for the &quot;Who-handled-me&quot; header
    counts.compute(whoHandledMe, (k, v) -&gt; v == null ? 1 : v + 1)
}
